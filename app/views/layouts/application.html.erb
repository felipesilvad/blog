<!DOCTYPE html>
<html>
  <head>
    <title>Blog</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>


  <body>
    <table>
      <tr>
      <% if user_signed_in? %>
        <td class="item"><%= link_to('Logout', destroy_user_session_path, method: :delete) %></td>
      <% else %>
        <td class="item"><%= link_to "Sign Up", new_user_registration_path %></td>
        <td class="item"> <%= link_to('Login', new_user_session_path) %></td>
      <% end %>
      </tr>
    </table>
    
    <%= yield %>
  </body>

  
  
</html>
